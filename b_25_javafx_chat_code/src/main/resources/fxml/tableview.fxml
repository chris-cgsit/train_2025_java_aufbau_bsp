<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="at.cgsit.a_jfxintro.table.TableController"
            prefHeight="400.0" prefWidth="600.0">

    <TableView fx:id="tableView" onMouseClicked="#mouseClicked" >
        <columns>
            <TableColumn fx:id="colFirst" text="First Name" />
            <TableColumn fx:id="colLast"  text="Last Name" />
            <TableColumn fx:id="colAge"   text="Age" />
        </columns>
    </TableView>


</AnchorPane>
